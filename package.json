{
  "name": "bunbury-storefront",
  "version": "2.0.13",
  "description": "E-commerce Storefront",
  "resolutions": {
    "react-dev-utils": "12.0.0-next.47"
  },
  "scripts": {
    "upgrade-deps": "yarn upgrade-interactive --latest",
    "generateTypes": "node ./src/lib/apollo/getPossibleTypes.js",
    "dev": "yarn clean-cache && next",
    "start": "next start",
    "clean-babel-cache": "rimraf './node_modules/.cache/babel-loader'",
    "clean": "rimraf -fr .next",
    "clean-cache": "rimraf -rf './node_modules/.cache'",
    "extract-intl": "node ./scripts/extract-intl.js",
    "build": "cross-env BUILD_ENV=production next build && yarn extract-intl",
    "build:production": "yarn clean-babel-cache && yarn build && yarn --production",
    "lint": "next lint",
    "test:ci": "run-s lint test",
    "test": "cross-env NODE_ENV=test jest --runInBand --colors $@",
    "test:watch": "yarn test --watch",
    "test:watch:verbose": "yarn test --watch --verbose false",
    "jest:update": "cross-env NODE_ENV=test jest --runInBand --updateSnapshot --colors $@",
    "jest:clean": "jest --clearCache",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage=true --runInBand --colors $@",
    "prettify": "prettier --write",
    "release": "standard-version",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:ci": "start-server-and-test dev http-get://localhost:3000 cy:run",
    "analyze": "ANALYZE=true yarn build",
    "postbuild": "next-sitemap --config next-sitemap-config.js"
  },
